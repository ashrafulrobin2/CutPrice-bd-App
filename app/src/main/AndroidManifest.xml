<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eomsbd.cutprice">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.eomsbd.cutprice.adapters.network.CustomApplication"
        android:allowBackup="true"
        android:icon="@drawable/cutpricelogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cutpricelogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.eomsbd.cutprice.adapters.Test_Activity"/>




        <activity
            android:name="com.eomsbd.cutprice.adapters.SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name="com.eomsbd.cutprice.adapters.ShoppingActivity">

        </activity>
        <activity android:name="com.eomsbd.cutprice.adapters.ProductActivity" android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.eomsbd.cutprice.adapters.CheckoutActivity" />
        <activity android:name="com.eomsbd.cutprice.adapters.PaymentsActivity" />
        <activity android:name="com.eomsbd.cutprice.adapters.PayPalCheckoutActivity" />

        <activity android:name="com.eomsbd.cutprice.adapters.OrderNowActivity" android:theme="@style/AppTheme.NoActionBar">



        </activity>
    </application>

</manifest>